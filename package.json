{
  "name": "weaveroo-components",
  "version": "1.0.16",
  "description": "Weaveroo React component library using Rollup,JS,Sass,Css,Tailwind and Storybook with Playroom",
  "main": "dist/index.js",
  "module": "dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006  --no-manager-cache",
    "build-storybook": "build-storybook",
    "playroom:start": "weaverplay start",
    "playroom:build": "weaverplay build",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "build-lib:dev": "rm -rf dist && rollup -c && npm run build:style",
    "build-lib:prod": "rm -rf dist && rollup -c --environment INCLUDE_DEPS,BUILD:production && npm run build:style && publish:asset",
    "build:style": "tailwindcss build -i src/style/tailwind.css -o dist/tailwind.css",
    "publish:asset": "aws s3 cp dist s3://weaver-testing/component-lib/ --recursive --exclude '*' --include '*.css'"
  },
  "keywords": [
    "React",
    "Component",
    "Library",
    "Rollup",
    "JS",
    "Css",
    "Tailwind",
    "Sass",
    "Storybook",
    "Playroom"
  ],
  "author": "Thirunavukarasan",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@storybook/addon-actions": "^6.4.0-alpha.30",
    "@storybook/addon-essentials": "^6.4.0-alpha.30",
    "@storybook/addon-links": "^6.4.0-alpha.30",
    "@storybook/addon-measure": "^6.4.0-alpha.30",
    "@storybook/addon-outline": "^6.4.0-alpha.30",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.4.0-alpha.30",
    "@storybook/theming": "^6.4.0-alpha.30",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "css-in-js-loader": "^0.1.2",
    "css-loader": "^5.2.6",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "playroom": "^0.25.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^4.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.52.2",
    "rollup-plugin-css-bundle": "^1.0.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.35.1",
    "sass-loader": "^10.1.1",
    "storybook-addon-playroom": "^2.1.2",
    "style-loader": "^2.0.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.7"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "postcss-loader": "^4.3.0"
  },
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "concurrently": "^6.2.0",
    "dedent": "^0.7.0",
    "node-sass": "^6.0.0"
  }
}
